{
    "name": "stefanwiegmann/wiki-bundle",
    "type": "symfony-bundle",
    "description": "A wiki bundle for Symfony5",
    "authors" : [{
        "name" : "Stefan Wiegmann",
        "email" : "mail@stefanwiegmann.de"
    }],
    "keywords" : [
        "wiki bundle symfony symfony5"
    ],
    "license" : [
        "MIT"
    ],
    "require": {
        "php": "^7.2.5",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/mailer": "5.0.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/twig-pack": "*",
        "symfony/yaml": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "Stefanwiegmann\\WikiBundle\\": ""
        }
    },
    "extra" : {
    "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}
